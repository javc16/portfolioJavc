---
import type { companyLogo } from "../interfaces/interfaces";
const { logos } = Astro.props;

---


<div class="flex flex-col mb-10">
  <ul class="flex flex-wrap justify-around">
    {logos.map((logo: companyLogo) => (
      <li class="p-20 w-1/2 sm:w-1/3 lg:w-1/5">
        <img class="h-8" src={logo.url} alt={logo.name} />
      </li>
    ))}
  </ul>
</div>



